Enter Event(connect)
    Popup -> Background
        message = {
            from: "popup",
            action: "enter",
            data: {
                'username': username_element.value,
                'roomname': roomname_element.value,
                'secretkey': secretkey_element.value
            }
        }

    
    Background -> Socket( event = enter)
        data: {
                'username': username_element.value,
                'roomname': roomname_element.value,
                'secretkey': secretkey_element.value
            }

    Socket -> Background 
        message = {
            successful: true/false
            self : socketid
            room_detials: {    (basically all_rooms[roomname])
                video_details: {
                        video_title: ______,
                        favicon: ______,
                        link: _____
                }
                connected_users: {
                    socketid : name
                }
                count : 
            }
        }

    Background -> Popup

        message = {
            action: enter
            successful : true/false
            data: {
                room_detials: {
                    video_details: {
                            video_title: ______,         <- if Successful else
                            favicon: ______,                    reason : _______
                            link: _____
                    }
                    count : 
                }
            }
        }







